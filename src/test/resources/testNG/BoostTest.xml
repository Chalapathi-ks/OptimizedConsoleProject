<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="BoostTestSuite" verbose="10" preserve-order="true">

    <test name="Merch - BoostTest - boostingTest">
        <classes>
            <class name="UnbxdTests.testNG.consoleui.MerchConditionTest.BoostTest">
                <methods>
                    <include name="boostingTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Merch - BoostTest - globalBoostingTest">
        <classes>
            <class name="UnbxdTests.testNG.consoleui.MerchConditionTest.BoostTest">
                <methods>
                    <include name="globalBoostingTest"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
